{
    "name": "@synthql/root",
    "type": "module",
    "version": "0.37.4",
    "license": "MIT",
    "private": true,
    "workspaces": [
        "packages/*"
    ],
    "scripts": {
        "format:root": "yarn prettier --write .",
        "format": "yarn prettier --write .",
        "typecheck": "yarn nx run-many -t typecheck",
        "test:ci": "yarn nx run-many -t test:ci",
        "build": "yarn nx run-many -t build",
        "build:docs": "yarn nx build @synthql/docs",
        "compile-executable-examples": "yarn nx run-many -t compile-executable-examples",
        "ci": "yarn nx run-many -t typecheck test:ci build",
        "ci:force": "yarn nx run-many --skip-nx-cache -t typecheck test:ci build",
        "link-all": "yarn --cwd packages/backend link && yarn --cwd packages/queries link && yarn --cwd packages/react link",
        "pub": "node ./scripts/publish.cjs"
    },
    "devDependencies": {
        "nx": "18.2.1",
        "prettier": "^3.1.1"
    },
    "dependencies": {
        "typedoc": "^0.25.12"
    }
}
