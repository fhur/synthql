{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "test:ci": {
      "dependsOn": [
        "^test:ci"
      ],
      "cache": true
    },
    "lint": {
      "dependsOn": [
        "^lint"
      ],
      "cache": true
    },
    "format": {
      "dependsOn": [
        "^format"
      ],
      "cache": true
    },
    "build": {
      "dependsOn": [
        "^build", "test:ci", "lint", "format"
      ],
      "outputs": [
        "{projectRoot}/build"
      ],
      "cache": true
    },
    "typecheck": {
      "dependsOn": [
        "^typecheck"
      ],
      "cache": true
    }

  },
  "defaultBase": "master"
}
